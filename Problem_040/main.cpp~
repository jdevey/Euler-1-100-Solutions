//#pragma GCC optimize("03")

#include <iostream>
#include <fstream>
#include <algorithm>
#include <vector>
#include <cmath>
#include <cstring>

#define BIG 1000000
#define fl(x,n) for(int (x) = 0; (x) < (n); ++(x))

using namespace std;

typedef long long ll;

int main() {
	
	string s;
	fl (i, BIG + 1) s += to_string(i);

	int prod = 1;
	fl (i, 7) prod *= s[pow(10, i)] - '0';

	cout << prod;
	
    return 0;
}
